<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<VBox alignment="CENTER" prefHeight="500.0" prefWidth="1000.0" spacing="10.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.miage.toulouse.projetjavas6.ChoisirReferentController">
   <children>
      <Text fill="#f9cb9c" strokeType="OUTSIDE" strokeWidth="0.0" text="Choix réferént" VBox.vgrow="ALWAYS">
         <font>
            <Font size="23.0" />
         </font>
      </Text>
      <TableView fx:id="tabRef" prefHeight="617.0" prefWidth="1000.0" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn fx:id="mention" maxWidth="216.0" minWidth="110.0" prefWidth="142.0" text="Mention" />
            <TableColumn fx:id="nom" prefWidth="248.0" text="Nom" />
            <TableColumn fx:id="prenom" prefWidth="265.0" text="Prenom" />
            <TableColumn fx:id="numRef" prefWidth="258.0" text="Num réferént" />
            <TableColumn fx:id="voir" minWidth="0.0" prefWidth="217.0" text="Choix" />
         </columns>
         <padding>
            <Insets right="10.0" />
         </padding>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <VBox alignment="BOTTOM_LEFT" prefHeight="200.0" prefWidth="100.0">
         <children>
            <Button fx:id="annuler" alignment="BOTTOM_LEFT" mnemonicParsing="false" nodeOrientation="LEFT_TO_RIGHT" onAction="#retourPageEtu" prefHeight="36.0" style="-fx-background-color: #f9cb9c;" text="Annuler" textAlignment="CENTER" VBox.vgrow="ALWAYS">
               <font>
                  <Font size="15.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
